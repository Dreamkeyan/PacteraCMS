/*** layer模态框自定义样式 ***/
.layer-model{
    min-width: 320px;
    height: auto;
    padding: 20px;
    position: relative;
    /*overflow: hidden;*/
}
.layer-header{
    width: 100%;
    min-width: 320px;
    font-size: 16px;
    font-weight: bolder;
    line-height: 36px;
    height: 36px;
    background-color: #ddd;
    text-align: center;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    border-bottom: 1px solid #ddd;
}
.layer-header .title{
    float: left;
    padding-left: 15px;
}
.layer-content{
    position: relative;
    width: 100%;
    height: auto;
    min-width: 320px;
}
.layer-footer{
    padding: 19px 20px 20px;
    margin-top: 15px;
    text-align: right;
    border-top: 1px solid #e5e5e5;
}
.layer-model .form-group{
    margin: 10px;
}
.layer-model input,
.layer-model textarea{
    border-radius: 3px;
}


